{
  "name": "storefront",
  "version": "1.0.0",
  "description": "Build a storefront backend",
  "main": "index.js",
  "scripts": {
    "build": "npx tsc",
    "start": "nodemon src/index.ts",
    "jasmine": "jasmine",
    "lint": "eslint src --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "lint:fix": "eslint . --fix",
    "prettier": "prettier --check .",
    "prettier:fix": "prettier --write .",
    "migrate": "db-migrate --env test up && db-migrate up",
    "test": "npm run build && ENV=test db-migrate --env test up && jasmine && db-migrate db:drop test"
  },
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@types/express": "^4.17.21",
    "@types/jasmine": "^5.1.4",
    "@types/node": "^20.11.19",
    "@types/pg": "^8.11.4",
    "@types/supertest": "^6.0.2",
    "@typescript-eslint/eslint-plugin": "^7.0.2",
    "@typescript-eslint/parser": "^7.0.2",
    "dotenv": "^16.4.5",
    "eslint": "^8.56.0",
    "eslint-config-prettier": "^9.1.0",
    "jasmine": "^5.1.0",
    "jasmine-spec-reporter": "^7.0.0",
    "jasmine-ts": "^0.4.0",
    "nodemon": "^3.0.3",
    "prettier": "^3.2.5",
    "supertest": "^6.3.4",
    "ts-node": "^10.9.2",
    "typescript": "^5.3.3"
  },
  "dependencies": {
    "db-migrate": "^0.11.14",
    "db-migrate-pg": "^1.5.2",
    "express": "^4.18.2",
    "pg": "^8.11.3",
    "sharp": "^0.33.2"
  }
}
